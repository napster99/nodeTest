<%- include header %>


<div id="wrapper">
	<div id="main">
		<div id="sidebar">
			<%- include sidebar %>
		</div>
		<div id="content">
			<div class="panel">
				<div class="header">
					<ul class="breadcrumb">
						<li><a href="/">主页</a><span class="divider">/</span></li>
						<li class="active"><%= otheruser.name%></li>
					</ul>
				</div>
				<div class="inner userinfo">
					<div class="well">
						<span>手机号：</span>
						<span><%= otheruser.mobile%></span>
					</div>
					<div class="well">
						<span>电子邮件：</span>
						<span><%= otheruser.email %></span>
					</div>
					<div class="well">
						<span>个人主页：</span>
						<a href="<%= otheruser.url%>" target="_blank"><%= otheruser.url %></a>
					</div>
					
					<div class="well">
						<span>个人技能：</span>
						<span><%= otheruser.profile %></span>
					</div>
					<div class="well">
						<span>微博地址：</span>
						<span><%= otheruser.weibo %></span>
					</div>

					<p class="col_fade" style="margin:12px 5px 0;">于 2011-7-21 12:40 加入 BF前端社区</p>
				</div>
			</div>
		</div>
	</div>
</div>
<div class="clearfix"></div>
<%- include footer %>